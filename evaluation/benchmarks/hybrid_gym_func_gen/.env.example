# Environment variables for swe_doc_gen_func benchmark
# Copy this to .env and customize as needed

# Runtime type (docker or remote)
RUNTIME=docker

# Whether to include return type/value hints in the prompt
# When true, adds return type (from signature) and return examples (from body) to function_info
INCLUDE_RETURNS=false

# Whether to include GPT-4o-mini generated implementation instructions in the prompt
# Requires dataset to have 'implementation_instruction' field (run generate_instructions.py first)
# Default: true
INCLUDE_INSTRUCTION=true

# Enable/disable web browsing
RUN_WITH_BROWSING=false

# Enable/disable hint text
USE_HINT_TEXT=false

# Docker image for RepoST evaluation
# Default: yiqingxyq/repost:v0 (used by eval_func_completion.py)
REPOST_DOCKER_IMAGE=yiqingxyq/repost:v0
